#include<stdio.h>
        
        void main()
        {
            int sum=0,n,t,r;
              
             
              
              printf("Enter the number:");
              scanf("%d",&n);
              t=n;
               
               while(n>0)
               {
                   r=n%10;
                   sum=sum*10+r;
                   n=n/10;
               }
               n=t;
               if (n==sum)
               
                   printf("palindraome");
               
                   else
                   printf("not palindraome");
               
        }


